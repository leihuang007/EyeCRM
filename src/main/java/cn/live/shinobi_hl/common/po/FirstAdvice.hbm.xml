<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.live.shinobi_hl.common.po">
	<class name="FirstAdvice" table="MD_FIRST_ADVICE">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="firstAdvice" type="string" length="80" not-null="true"
			unique="true" />
		<property name="status" type="int" not-null="true"/>
	</class>
</hibernate-mapping>